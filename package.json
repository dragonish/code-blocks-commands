    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "builtin-modules": "5.0.0",
    "commit-and-tag-version": "^12.5.1",
    "esbuild": "^0.25.5",
    "eslint": "^9.30.1",
    "globals": "^16.3.0",
    "obsidian": "^1.8.7",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
